{
  // genre_id,genre_name,genre_name_lang,genre_type,
	"fields": [
		{
			"name": "genre_id",
			"constraints": {
				"required": true,
        "unique": true,
				"pattern": "G[0-9]{2}"
			}
		},
    {
      "name": "genre_name",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "genre_name_lang",
      "constraints": {
        "required": true,
        "min-length": 2
      }
    },
    {
      "name": "genre_type",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "source_link",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#anyURI"
      }
    },
    {
      "name": "created",
			"constraints": {
				"required": true,
				"type": "http://www.w3.org/2001/XMLSchema#date"
			}
    },
    {
      "name": "created_by",
			"constraints": {
				"required": true,
				"pattern": "[A-Z]{2}"
			}
    },
    {
      "name": "last_modified",
			"constraints": {
				"required": true,
				"type": "http://www.w3.org/2001/XMLSchema#date"
			}
    },
    {
      "name": "last_modified_by",
			"constraints": {
				"required": true,
				"pattern": "[A-Z]{2}"
			}
    }
	]
}
